    "KeyN": [ "n", "N" ],
    "KeyM": [ "m", "M" ],
    "Comma": [ ",", "<" ],
    "Period": [ ".", ">" ],
    "Slash": [ "/", "?" ],
    "Minus": [ "-", "_" ],
    "Equal": [ "=", "+" ],
    "BracketLeft": [ "[", "{" ],
    "BracketRight": [ "]", "}" ],
    "Quote": [ "'", "\"" ],
    "Backquote": [ "*₀", "*⁰" ],
    "Backslash": [ "\\", "|" ],
    "Space": [ " ", " " ]
  },
  "deadkeys": {
    "*₀": {"0" : "₀", "1" : "₁", "2": "₂", "3" : "₃", "4" : "₄", "5" : "₅", "6" : "₆", "7" : "₇", "8" : "₈", "9": "₉"},
