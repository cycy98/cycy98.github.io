{
  "name": "qwerty-us",
  "description": "American QWERTY layout",
  "geometry": "iso",
  "keymap": {
    "Digit1": [ "1", "~" ], 
    "Digit2": [ "2", "@" ],
    "Digit3": [ "3", "\"", "#" ],
    "Digit4": [ "4", "'", "$" ],
    "Digit5": [ "5", "%" ],
    "Digit6": [ "6", "^", "|" ],
    "Digit7": [ "7", "&" ],
    "Digit8": [ "8", "*" ],
    "Digit9": [ "9", "(", "{", "["],
    "Digit0": [ "0", ")", "}", "]" ],
    "KeyQ": [ "g", "G" ],
    "KeyW": [ "d", "D" ],
    "KeyE": [ "l", "L" ],
    "KeyR": [ "t", "T" ],
    "KeyT": [ "n", "N" ],
    "KeyY": [ "a", "A", "æ", "Æ" ],
    "KeyU": [ "p", "P" ],
    "KeyI": [ "é", "É" ],
    "KeyO": [ "z", "Z" ],
    "KeyP": [ "?", "\u2026" ],
    "KeyA": [ "u", "U" ],
    "KeyS": [ "k", "K" ],
    "KeyD": [ "c", "C" ],
    "KeyF": [ "h", "H" ],
    "KeyG": [ "e", "E" ],
    "KeyH": [ "s", "S" ],
    "KeyJ": [ "m", "M" ],
    "KeyK": [ "v", "V" ],
    "KeyL": [ "y", "Y" ],
    "Semicolon": [ "j", "J" ],
    "KeyZ": [ "q", "Q" ],
    "KeyX": [ "w", "W" ],
    "KeyC": [ "r", "R" ],
    "KeyV": [ "o", "O", "œ", "Œ" ],
    "KeyB": [ "i", "I" ],
    "KeyN": [ "è", "È" ],
    "KeyM": [ "f", "F" ],
    "Comma": [ "b", "B" ],
    "Period": [ "x", "X" ],
    "Slash": [ "ç", "Ç" ],
    "Minus": [ "-", "_", "×" ],
    "Equal": [ "=", "+", "÷" ],
    "BracketLeft": [ "!", "§"],
    "BracketRight": [ ":", ";" ],
    "Quote": [ "/", "\\" ],
    "Backquote": [ "*₀", "*⁰" ],
    "Backslash": [ ",", "." ],
    "IntlBackslash": [ "<", ">", "«", "»" ],
    "Space": [ " ", " " ]
  },
  "deadkeys": {
    "*₀": {"0" : "₀", "1" : "₁", "2": "₂", "3" : "₃", "4" : "₄", "5" : "₅", "6" : "₆", "7" : "₇", "8" : "₈", "9": "₉"},
    "*⁰": {"0" : "⁰", "1" : "¹", "2": "²", "3" : "³", "4" : "⁴", "5" : "⁵", "6" : "⁶", "7" : "⁷", "8" : "⁸", "9": "⁹"}
  },
  "altgr": true
}
